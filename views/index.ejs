<%- include("partials/header") -%>

    <div class="box-root padding-top--24 flex-flex flex-direction--column" style="flex-grow: 1; z-index: 9;">
        <div class="box-root padding-top--48 padding-bottom--24 flex-flex flex-justifyContent--center">
            <h1><a href="" rel="dofollow">Continental Cloud</a></h1>
        </div>
        <div class="formbg-outer">
            <div class="formbg">
                <div class="formbg-inner padding-horizontal--48">
                    <span class="padding-bottom--15">Clean your data here</span>
                    <form id="stripe-login" method="post" enctype="multipart/form-data" action="/upload">
                        <div class="field padding-bottom--24">
                            <input type="text" name="title" placeholder="Optional Title">
                        </div>
                        <div class="field padding-bottom--24">
                            <input type="file" name="upl" multiple />
                        </div>
                        <div class="field padding-bottom--24">
                            <input type="submit" name="submit" value="Continue">
                        </div>
                    </form>
                </div>
            </div>
            <div class="footer-link padding-top--24">
                <span> Our promise : - Security and Speed</span>
                <%- include("partials/footer") -%>